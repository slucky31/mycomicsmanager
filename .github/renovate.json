{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":label(renovate)",
    ":separateMultipleMajorReleases",    
    ":semanticCommitTypeAll(fix)"
  ],
  "prConcurrentLimit": 10,
  "baseBranches": ["main","net6"],
  "packageRules": [
    {
      "matchUpdateTypes": ["major"],
      "matchBaseBranches": ["main","net6"],
      "enabled": false
    }
  ]

}